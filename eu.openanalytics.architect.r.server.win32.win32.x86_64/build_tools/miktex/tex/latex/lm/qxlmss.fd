\ProvidesFile{qxlmss.fd}[2009/10/30 v1.6 Font defs for Latin Modern]
\DeclareFontFamily{QX}{lmss}{}
\DeclareFontShape{QX}{lmss}{m}{n}
     {<-8.5> qx-lmss8
      <8.5-9.5> qx-lmss9      <9.5-11>  qx-lmss10
      <11-15.5> qx-lmss12     <15.5-> qx-lmss17
      }{}
\DeclareFontShape{QX}{lmss}{m}{it}
      {<->ssub*lmss/m/sl}{}
\DeclareFontShape{QX}{lmss}{m}{sl}
     {<-8.5> qx-lmsso8
      <8.5-9.5> qx-lmsso9      <9.5-11>  qx-lmsso10
      <11-15.5> qx-lmsso12     <15.5-> qx-lmsso17
      }{}
%%%%%%% Font/shape undefined, therefore substituted
\DeclareFontShape{QX}{lmss}{m}{sc}
     {<->sub*lmr/m/sc}{}
\DeclareFontShape{QX}{lmss}{b}{n}
     {<->ssub * lmss/bx/n}{}
\DeclareFontShape{QX}{lmss}{b}{sl}
     {<->ssub * lmss/bx/sl}{}
\DeclareFontShape{QX}{lmss}{b}{it}
     {<->ssub * lmss/bx/it}{}
%%%%%%%% semibold condensed series
\DeclareFontShape{QX}{lmss}{sbc}{n}
     {<-> qx-lmssdc10}{}
\DeclareFontShape{QX}{lmss}{sbc}{sl}
     {<-> qx-lmssdo10}{}
\DeclareFontShape{QX}{lmss}{sbc}{it}
     {<->ssub*lmss/sbc/sl}{}
%%%%%%%%% bold extended series
\DeclareFontShape{QX}{lmss}{bx}{n}
     {<-> qx-lmssbx10}{}
\DeclareFontShape{QX}{lmss}{bx}{sl}
     {<-> qx-lmssbo10}{}
\DeclareFontShape{QX}{lmss}{bx}{it}
     {<->ssub*lmss/bx/sl}{}

\ProvidesFile{l7xlmtt.fd}[2009/10/30 v1.6 Font defs for Latin Modern]
\DeclareFontFamily{L7x}{lmtt}{\hyphenchar \font\m@ne}

\ifx\lmtt@use@light@as@normal\@empty
% macro defined, so we use the light variant as medium (m), and
% medium as semi-bold (sb):
\DeclareFontShape{L7x}{lmtt}{sb}{n}
     {<-8.5>   l7x-lmtt8     <8.5-9.5> l7x-lmtt9
      <9.5-11> l7x-lmtt10    <11->     l7x-lmtt12
      }{}
\DeclareFontShape{L7x}{lmtt}{sb}{it}
     {<-> l7x-lmtti10}{}
\DeclareFontShape{L7x}{lmtt}{sb}{sl}
     {<-> l7x-lmtto10}{}
\DeclareFontShape{L7x}{lmtt}{sb}{sc}
     {<-> l7x-lmtcsc10}{}
\DeclareFontShape{L7x}{lmtt}{sb}{scsl}
     {<-> l7x-lmtcso10}{}
%%%%%%%% light (l), light condensed (lc), and dark (b) variants:
\DeclareFontShape{L7x}{lmtt}{m}{n}
     {<-> l7x-lmtl10}{}
\DeclareFontShape{L7x}{lmtt}{m}{it}
     {<->sub*lmtt/l/sl}{}
\DeclareFontShape{L7x}{lmtt}{m}{sl}
     {<-> l7x-lmtlo10}{}
\DeclareFontShape{L7x}{lmtt}{c}{n}
     {<-> l7x-lmtlc10}{}
\DeclareFontShape{L7x}{lmtt}{c}{it}
     {<->sub*lmtt/lc/sl}{}
\DeclareFontShape{L7x}{lmtt}{c}{sl}
     {<-> l7x-lmtlco10}{}
\else
% usual setup of variants:
\DeclareFontShape{L7x}{lmtt}{m}{n}
     {<-8.5>   l7x-lmtt8     <8.5-9.5> l7x-lmtt9
      <9.5-11> l7x-lmtt10    <11->     l7x-lmtt12
      }{}
\DeclareFontShape{L7x}{lmtt}{m}{it}
     {<-> l7x-lmtti10}{}
\DeclareFontShape{L7x}{lmtt}{m}{sl}
     {<-> l7x-lmtto10}{}
\DeclareFontShape{L7x}{lmtt}{m}{sc}
     {<-> l7x-lmtcsc10}{}
\DeclareFontShape{L7x}{lmtt}{m}{scsl}
     {<-> l7x-lmtcso10}{}
%%%%%%%% light (l), light condensed (lc), and dark (b) variants:
\DeclareFontShape{L7x}{lmtt}{l}{n}
     {<-> l7x-lmtl10}{}
\DeclareFontShape{L7x}{lmtt}{l}{it}
     {<->sub*lmtt/l/sl}{}
\DeclareFontShape{L7x}{lmtt}{l}{sl}
     {<-> l7x-lmtlo10}{}
\DeclareFontShape{L7x}{lmtt}{lc}{n}
     {<-> l7x-lmtlc10}{}
\DeclareFontShape{L7x}{lmtt}{lc}{it}
     {<->sub*lmtt/lc/sl}{}
\DeclareFontShape{L7x}{lmtt}{lc}{sl}
     {<-> l7x-lmtlco10}{}
\fi
% bold is always bold (b):
\DeclareFontShape{L7x}{lmtt}{b}{n}
     {<-> l7x-lmtk10}{}
\DeclareFontShape{L7x}{lmtt}{b}{it}
     {<->sub*lmtt/b/sl}{}
\DeclareFontShape{L7x}{lmtt}{b}{sl}
{<-> l7x-lmtko10}{}
\DeclareFontShape{L7x}{lmtt}{bx}{it}
     {<->sub*lmtt/b/sl}{}
\DeclareFontShape{L7x}{lmtt}{bx}{n}
     {<->ssub*lmtt/b/n}{}
\DeclareFontShape{L7x}{lmtt}{bx}{sl}
     {<->ssub*lmtt/b/sl}{}
\endinput

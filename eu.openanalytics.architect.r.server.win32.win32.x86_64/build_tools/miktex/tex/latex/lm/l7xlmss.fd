     {<-8.5> l7x-lmsso8
      <8.5-9.5> l7x-lmsso9      <9.5-11>  l7x-lmsso10
      <11-15.5> l7x-lmsso12     <15.5-> l7x-lmsso17
      }{}
%%%%%%% Font/shape undefined, therefore substituted
\DeclareFontShape{L7x}{lmss}{m}{sc}
     {<->sub*lmr/m/sc}{}
\DeclareFontShape{L7x}{lmss}{b}{n}
     {<->ssub * lmss/bx/n}{}
\DeclareFontShape{L7x}{lmss}{b}{sl}
     {<->ssub * lmss/bx/sl}{}
\DeclareFontShape{L7x}{lmss}{b}{it}
     {<->ssub * lmss/bx/it}{}
%%%%%%%% semibold condensed series
\DeclareFontShape{L7x}{lmss}{sbc}{n}

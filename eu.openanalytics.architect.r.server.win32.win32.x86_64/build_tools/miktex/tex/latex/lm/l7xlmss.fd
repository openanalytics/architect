\DeclareFontShape{L7x}{lmss}{sbc}{sl}
     {<-> l7x-lmssdo10}{}
\DeclareFontShape{L7x}{lmss}{sbc}{it}
     {<->ssub*lmss/sbc/sl}{}
%%%%%%%%% bold extended series
\DeclareFontShape{L7x}{lmss}{bx}{n}

\ProvidesFile{il2lmss.fd}[2009/10/30 v1.6 Font defs for Latin Modern]
\DeclareFontFamily{IL2}{lmss}{}
\DeclareFontShape{IL2}{lmss}{m}{n}
     {<-8.5> cs-lmss8
      <8.5-9.5> cs-lmss9      <9.5-11>  cs-lmss10
      <11-15.5> cs-lmss12     <15.5-> cs-lmss17
      }{}
\DeclareFontShape{IL2}{lmss}{m}{it}
      {<->ssub*lmss/m/sl}{}
\DeclareFontShape{IL2}{lmss}{m}{sl}
     {<-8.5> cs-lmsso8
      <8.5-9.5> cs-lmsso9      <9.5-11>  cs-lmsso10
      <11-15.5> cs-lmsso12     <15.5-> cs-lmsso17
      }{}
%%%%%%% Font/shape undefined, therefore substituted
\DeclareFontShape{IL2}{lmss}{m}{sc}
     {<->sub*lmr/m/sc}{}
\DeclareFontShape{IL2}{lmss}{b}{n}
     {<->ssub * lmss/bx/n}{}
\DeclareFontShape{IL2}{lmss}{b}{sl}
     {<->ssub * lmss/bx/sl}{}
\DeclareFontShape{IL2}{lmss}{b}{it}
     {<->ssub * lmss/bx/it}{}
%%%%%%%% semibold condensed series
\DeclareFontShape{IL2}{lmss}{sbc}{n}
     {<-> cs-lmssdc10}{}
\DeclareFontShape{IL2}{lmss}{sbc}{sl}
     {<-> cs-lmssdo10}{}
\DeclareFontShape{IL2}{lmss}{sbc}{it}
     {<->ssub*lmss/sbc/sl}{}
%%%%%%%%% bold extended series
\DeclareFontShape{IL2}{lmss}{bx}{n}
     {<-> cs-lmssbx10}{}
\DeclareFontShape{IL2}{lmss}{bx}{sl}
     {<-> cs-lmssbo10}{}
\DeclareFontShape{IL2}{lmss}{bx}{it}
     {<->ssub*lmss/bx/sl}{}

  \bbl@cs{renew@\bbl@section}%
  \global\bbl@csarg\let{renew@\bbl@section}\relax
  \bbl@cs{secpost@\bbl@section}%
  % The previous code belongs to the previous section.
  % --------------------------
  % Now start the current one.
  \in@{=date.}{=#1}%
  \ifin@
    \lowercase{\def\bbl@tempa{=#1=}}%
    \bbl@replace\bbl@tempa{=date.gregorian}{}%
    \bbl@replace\bbl@tempa{=date.}{}%

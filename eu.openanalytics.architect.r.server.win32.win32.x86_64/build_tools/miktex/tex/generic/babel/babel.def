    \ifx\bbl@tempa\relax\else
      \bbl@replace\bbl@tempa{=}{}%
      \bbl@exp{%
        \def\<bbl@inikv@#1>####1=####2\\\@@{%
          \\\bbl@inidate####1...\relax{####2}{\bbl@tempa}}}%
    \fi
  \fi
  \def\bbl@section{#1}%
  \def\bbl@elt##1##2{%
    \@namedef{bbl@KVP@#1/##1}{}}%

\def\getImgFileParser#1/{%
  \ifx\relax#1\relax
    \let\next\relax
  \else
    \do{#1}\let\next\getImgFileParser
  \fi\next
}
\newcommand\getImgFile[2]{%
  \def\do##1{\def\imgBaseName{##1#2}\def\imgBase{##1}}
  \def\imgFullName{#1#2}
  \getImgFileParser#1//
}


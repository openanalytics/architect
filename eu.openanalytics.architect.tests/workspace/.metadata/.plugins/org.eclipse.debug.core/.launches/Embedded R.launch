<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<launchConfiguration type="org.eclipse.statet.r.launchConfigurations.RConsole">
<stringAttribute key="org.eclipse.statet.r.debug/RConsole/arguments.options" value="--no-save --no-restore"/>
<booleanAttribute key="org.eclipse.statet.r.debug/RConsole/console.pin" value="false"/>
<stringAttribute key="org.eclipse.statet.r.debug/RConsole/init.script.snippet" value="# to avoid issue with transparency in ggplot2 with png in Architect&#10;options(bitmapType = &quot;cairo&quot;)"/>
<stringAttribute key="org.eclipse.statet.r.debug/RConsole/type" value="local.rjs-rmi"/>
<stringAttribute key="org.eclipse.statet.r.debug/REnv/REnvSetting" value="eplugin-local-eu.openanalytics.r.server;Embedded R Server"/>
<stringAttribute key="org.eclipse.statet.r.debug/REnv/workingDirectory" value="${architect_wd}"/>
<stringAttribute key="org.eclipse.statet.r/renv/REnvCode" value="eplugin-local-eu.openanalytics.r.server;Embedded R Server"/>
<stringAttribute key="org.eclipse.statet.r/renv/WorkingDirectory" value="${architect_wd}"/>
<mapAttribute key="org.eclipse.debug.core.environmentVariables">
<mapEntry key="JAVA_HOME" value="/opt/architect/architect-unstable/jre/jre"/>
<mapEntry key="LD_LIBRARY_PATH" value="/opt/architect/architect-unstable/jre/jre/lib/amd64:/opt/architect/architect-unstable/jre/jre/lib/amd64/server:${env_var:LD_LIBRARY_PATH}"/>
<mapEntry key="PATH" value="${architect_pandoc}:${env_var:PATH}"/>
<mapEntry key="R_KEEP_PKG_SOURCE" value="yes"/>
<mapEntry key="R_ZIPCMD" value="zip"/>
</mapAttribute>
<booleanAttribute key="org.eclipse.debug.ui.ATTR_CONSOLE_OUTPUT_ON" value="false"/>
<listAttribute key="org.eclipse.debug.ui.favoriteGroups">
<listEntry value="org.eclipse.debug.ui.launchGroup.debug"/>
<listEntry value="org.eclipse.debug.ui.launchGroup.run"/>
</listAttribute>
<stringAttribute key="process_factory_id" value="org.eclipse.statet.nico.LogRuntimeProcessFactory"/>
</launchConfiguration>
